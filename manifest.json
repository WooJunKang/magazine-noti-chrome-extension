{
  "name": "magazine notification(test)",
  "version": "1.0",
  "description": "this is a notification extension when there is a new article from urclass",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": [
      "./src/background.js"
    ]
  },
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_title": "CodeStates Magazine",
    "default_popup": "popup.html",
    "default_icon": "./public/codestates_logo.png"
  }
}